@model IEnumerable<Nave>

@{
    ViewData["Title"] = "Naves";
}

<h1>Lista das Naves</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th scope="col">Nome</th>
            <th scope="col">Afiliação</th>
            <th scope="col">Classe</th>
            <th scope="col">Data de Lançamento (beta)</th>
            <th scope="col">Registro</th>
            <th scope="col">Status</th>
            <th scope="col">Propulsão</th>
            <th scope="col">Armamento</th>
            <th scope="col">Defesa</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
           <tr>
                <th scope="row">@obj.Nome</th>
                <td>@obj.Afiliacao</td>
                <td>@obj.Classe</td>
                <td>@obj.DataLancamento</td>
                <td>@obj.Registro</td>
                <td>@obj.Status</td>
                <td>@obj.Propulsao</td>
                <td>@obj.Armamento</td>
                <td>@obj.Defesa</td>
            </tr>   
        }
    </tbody>
</table>